set nu
syntax enable 
set nocompatible
filetype off
set hlsearch
set backspace=indent,eol,start
set showcmd
set ttyfast
set title
set cc=80
set autoindent
set mouse=a
set hidden
set cursorline
filetype plugin indent on
set completeopt=noinsert,menuone,noselect
set clipboard=unnamedplus
set encoding=utf8
set noeb
set novb
set showmatch
set ruler
set smarttab
set shiftwidth=4
set tabstop=4

let python_highlight_all=1
